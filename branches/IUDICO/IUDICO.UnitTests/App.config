<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <connectionStrings>
    <add name="IUDICO-TEST" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=IUDICO-TEST;Integrated Security=True" />
  </connectionStrings>
  <appSettings>
    <add key="SELENIUM_HOST" value="localhost" />
    <add key="SELENIUM_PORT" value="4444" />
