    <add key="SELENIUM_URL" value="http://localhost:1569/" />
    <add key="SELENIUM_SPEED" value="1000" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
	 <add key="PathToIUDICO.UnitTests" value="C:\Users\Administrator\.hudson\jobs\IUDICO\workspace\" />
