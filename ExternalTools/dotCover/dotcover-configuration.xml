<?xml version="1.0" encoding="utf-8"?>
<AnalyseParams>

  <TargetExecutable><!-- File name of the program to analyse. -->C:\Program Files (x86)\NUnit 2.6\bin\nunit-console-x86.exe</TargetExecutable>
  <TargetArguments>IUDICO.UnitTests\bin\Debug\IUDICO.UnitTests.dll</TargetArguments>
  <TargetWorkingDir><!-- Program working directory. --></TargetWorkingDir>
  <TempDir><!-- Program working directory. --></TempDir>
  <Output><!-- Program working directory. -->\CodeCoverageReport.html</Output>
  <ReportType><!-- [HTML|JSON|XML|NDependXML]. A type of the report. XML by default. -->HTML</ReportType>
  <InheritConsole><!-- [True|False] Lets the application being analysed to inherit dotCover console. True by default. --> </InheritConsole>  
  
  <Filters>

    <IncludeFilters>

      <FilterEntry>
        <ModuleMask>*</ModuleMask>
        <ClassMask>*</ClassMask>
        <FunctionMask>*</FunctionMask>
      </FilterEntry>

    </IncludeFilters>

    <ExcludeFilters>

      <FilterEntry>
        <ModuleMask>IUDICO.UnitTests</ModuleMask>
        <ClassMask>*</ClassMask>
        <FunctionMask>*</FunctionMask>
      </FilterEntry>

    </ExcludeFilters>

  </Filters>

</AnalyseParams>
